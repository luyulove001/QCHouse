<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:id="@+id/out">
        <shape
            android:shape="oval">

            <solid android:color="@color/white_fafaf8"/>

            <stroke
                android:width="@dimen/x2"
                android:color="@color/colorRed"/>
            <size
                android:width="@dimen/x30"
                android:height="@dimen/y30"/>

        </shape>
    </item>

    <item
        android:id="@+id/in"
        android:bottom="@dimen/x6"
        android:left="@dimen/x6"
        android:right="@dimen/x6"
        android:top="@dimen/x6">
        <shape android:shape="oval">
            <solid android:color="@color/colorRed"/>
        </shape>
    </item>
</layer-list>
